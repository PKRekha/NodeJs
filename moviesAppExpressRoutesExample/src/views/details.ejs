<h1>This is details page about, movies</h1>
            <%for(var i=0; i<movies.length ;i++){%>
            <div class="row">
             <div class="col-md-10 col-sm-9">
                <h3><%=movies[i].name%></h3>
                <div class="row">
           
                    <h4><span class="label label-default">Movie Genre: <%=movies[i].type%></span></h4>
               
                      <h4 style="font-family:courier,'new courier';" class="text-muted">Rating: <%=movies[i].rate%> </h4>
                </div>
              </div>
            </div>

<%}%>